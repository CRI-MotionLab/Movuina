<div id="osc-config" class="config">
  <div class="param-container">
    target IP :
    <input type="number" class="ip-1" value="<%= ip[0] %>">
    <input type="number" class="ip-2" value="<%= ip[1] %>">
    <input type="number" class="ip-3" value="<%= ip[2] %>">
    <input type="number" class="ip-4" value="<%= ip[3] %>">
  </div>
  <div class="param-container">
    target port :
    <input type="number" class="port" value="<%= port %>">
  </div>
</div>